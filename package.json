{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch '**/*.ts' --exec \"ts-node --require tsconfig-paths/register\" src/bin/www.ts",
    "release": "node build/bin/www.js",
    "build": "tsc --project ./",
    "test": "env TS_NODE_PROJECT=\"tsconfig.json\" nyc mocha --exit --require ts-node/register --require tsconfig-paths/register test/**/*.ts",
    "sp_update": "nodemon --watch '**/*.ts' --exec \"ts-node --require tsconfig-paths/register\" src/sql/backup_procedure.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.4",
    "@grpc/proto-loader": "^0.6.4",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jimp": "^0.2.28",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^8.2.3",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.3.1",
    "@types/passport": "^1.0.7",
    "@types/passport-kakao": "^0.2.1",
    "@types/supertest": "^2.0.11",
    "aws-sdk": "^2.968.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "^1.6.1",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-kakao": "^1.0.1",
    "pug": "2.0.0-beta11",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-unicorn": "^35.0.0",
    "mocha": "^5.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^11.1.2",
    "supertest": "^3.0.0",
    "ts-sinon": "^2.0.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "import/no-unresolved": "off",
      "unicorn/filename-case": "off"
    }
  }
}
